{
  "name": "@permify-toolkit/nestjs",
  "type": "module",
  "version": "1.0.0",
  "description": "NestJS integration for Permify: Decorators, guards, and zero-config module setup.",
  "author": "Nikhil aka thisisnkc",
  "license": "MIT",
  "homepage": "https://github.com/thisisnkc/permify-toolkit/tree/main/packages/nestjs#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/thisisnkc/permify-toolkit.git",
    "directory": "packages/nestjs"
  },
  "bugs": {
    "url": "https://github.com/thisisnkc/permify-toolkit/issues"
  },
  "keywords": [
    "permify",
    "nestjs",
    "nestjs-module",
    "nestjs-guard",
    "authorization",
    "access-control",
    "rbac",
    "rebac",
    "permissions",
    "authorization-guard",
    "decorators",
    "multi-tenant"
  ],
  "exports": {
    ".": {
      "types": "./dist/nestjs/src/index.d.ts",
      "import": "./dist/nestjs/src/index.js"
    }
  },
  "main": "dist/nestjs/src/index.js",
  "types": "dist/nestjs/src/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "cross-env NODE_NO_WARNINGS=1 npx tsx ../../tests/setup.ts",
    "build": "tsc",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.12",
    "@nestjs/core": "^11.1.12",
    "@permify-toolkit/core": "workspace:^",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/common": "^11.1.12",
    "@nestjs/core": "^11.1.12",
    "@nestjs/testing": "^11.1.12",
    "rxjs": "^7.8.1",
    "ts-node": "^10.x",
    "typescript": "^4.x"
  }
}
