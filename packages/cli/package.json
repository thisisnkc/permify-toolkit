{
  "name": "@permify-toolkit/cli",
  "type": "module",
  "version": "1.0.0",
  "description": "cli pacakge for permify-toolkit",
  "author": "Nikhil aka thisisnkc",
  "license": "MIT",
  "keywords": [
    "cli",
    "permify-toolkit"
  ],
  "main": "dist/index.js",
  "bin": {
    "permify-toolkit": "bin/permify-toolkit"
  },
  "files": [
    "bin",
    "dist",
    "oclif.manifest.json"
  ],
  "oclif": {
    "bin": "permify-toolkit",
    "dirname": "permify-toolkit",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix",
    "test": "cross-env NODE_NO_WARNINGS=1 npx tsx ../../tests/setup.ts",
    "build": "tsc",
    "prepack": "oclif manifest && oclif readme",
    "postpack": "rm -f oclif.manifest.json",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^4.8.0",
    "@oclif/plugin-help": "^6.2.26",
    "@permify-toolkit/core": "workspace:^",
    "jiti": "^2.6.1",
    "oclif": "^4.22.63"
  },
  "devDependencies": {
    "ts-node": "^10.x",
    "typescript": "^4.x"
  }
}
