{
  "name": "@permify-toolkit/core",
  "type": "module",
  "version": "1.0.0",
  "description": "core package for permify-toolkit",
  "author": "Nikhil aka thisisnkc",
  "license": "MIT",
  "keywords": [
    "core",
    "permify-toolkit"
  ],
  "exports": {
    ".": {
      "types": "./types/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "main": "dist/src/index.js",
  "types": "./types/src/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "test": "cross-env NODE_NO_WARNINGS=1 npx tsx ../../tests/setup.ts",
    "test:file": "cross-env NODE_NO_WARNINGS=1 npx tsx ../../tests/setup.ts --files",
    "test:group": "cross-env NODE_NO_WARNINGS=1 npx tsx ../../tests/setup.ts --groups",
    "test:grep": "cross-env NODE_NO_WARNINGS=1 npx tsx ../../tests/setup.ts --tests",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix",
    "build": "tsc"
  },
  "dependencies": {
    "@permify/permify-node": "^1.1.4",
    "jiti": "^2.6.1"
  },
  "devDependencies": {
    "ts-node": "^10.x",
    "typescript": "^4.x"
  }
}
